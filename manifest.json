{
    "name": "Twitter-ImageExpander",
    "version": "2.1.1",
    "manifest_version": 2,
    "description": "Expand image size in Twitter",
    "icons": {
        "128": "128.png"
    },
    "content_scripts": [{
        "matches": [
            "https://twitter.com/*"
        ],
        "js": [
            "content.js"
        ]
    }]
}